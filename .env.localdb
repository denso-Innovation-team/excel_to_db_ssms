# Excel to SSMS - LocalDB Configuration (No Port Required)
DB_HOST=(LocalDB)\MSSQLLocalDB
DB_NAME=excel_to_db
DB_USER=
DB_PASSWORD=
DB_TYPE=mssql
DB_DRIVER=ODBC Driver 17 for SQL Server
DB_TRUSTED_CONNECTION=yes

# Connection Pool Settings
POOL_SIZE=5
MAX_OVERFLOW=10
POOL_TIMEOUT=30
POOL_RECYCLE=3600

# Processing Configuration
BATCH_SIZE=2000
MAX_WORKERS=4
CHUNK_SIZE=10000

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/excel_to_ssms.log
